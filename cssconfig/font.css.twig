{% if(fontsArray is defined) %}
    {% for fonts in fontsArray %}
        @import url(https://fonts.googleapis.com/css?family={{ fonts|raw }});
    {% endfor %}
{% endif %} 

/*font-family: 'PT Sans', sans-serif;*/
.fg-web-theme  .fg-body-bg header *, 
.fg-web-theme  .fg-body-bg main  *, 
.fg-web-theme  .fg-body-bg footer  *,
.fg-body-ckeditor.fg-web-theme * {
  /*----------------------------------------------------------------
  Main text style related elements
  -------------------------------------------------------------------*/
{% if(font.TM_MAIN_TEXT.fontName is defined and font.TM_MAIN_TEXT.fontName is not empty) %}
    font-family: {{ font.TM_MAIN_TEXT.fontName|raw }};
{% endif %}
{% if(font.TM_MAIN_TEXT.fontStrength is defined and font.TM_MAIN_TEXT.fontStrength is not empty) %}
    font-weight: {{ font.TM_MAIN_TEXT.fontStrength == 'lighter' ? '300' : font.TM_MAIN_TEXT.fontStrength|raw }};
{% endif %}
{% if( font.TM_MAIN_TEXT.isItalic is defined and font.TM_MAIN_TEXT.isItalic == 1 ) %}
    font-style: italic;
{% else %}
    font-style: normal;
{% endif %} 
{% if( font.TM_MAIN_TEXT.isUppercase is defined and font.TM_MAIN_TEXT.isUppercase == 1 ) %}
    text-transform: uppercase;
{% else %}
    text-transform: none;
{% endif %}

  /* main text style override on  other elements */
  
  /*----------------------------------------------------------------
  header section 
  -------------------------------------------------------------------*/
  /* heading style override on  other elements */
  /*----------------------------------------------------------------
  nav sectionstyle related elements
  -------------------------------------------------------------------*/
  /*----------------------------------------------------------------
  Main text style related elements
  -------------------------------------------------------------------*/
  /*other overide section */
}

